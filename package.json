  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "export NODE_OPTIONS=--openssl-legacy-provider; ng serve",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider; ng build --prod --build-optimizer",
    "budget": "ng build --prod --build-optimizer --vendor-chunk --common-chunk",
